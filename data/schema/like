Create table [like](
  id BIGINT IDENTITY(1,1) PRIMARY KEY,
  movie_id BIGINT NOT NULL,
  client_id BIGINT NOT NULL
  );

ALTER TABLE [like]
ADD FOREIGN KEY (client_id) REFERENCES client(id)

ALTER TABLE [like]
ADD FOREIGN KEY (movie_id) REFERENCES movie(id)

ALTER TABLE [like]
ADD CONSTRAINT unique_like_for_movie UNIQUE (movie_id, client_id);